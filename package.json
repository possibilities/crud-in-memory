{
  "name": "crud-in-memory",
  "description": "CRUD interface for in-memory data",
  "version": "0.0.4",
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "url": "https://github.com/possibilities/crud-in-memory",
    "type": "git"
  },
  "author": "Mike Bannister <mikebannister@gmail.com>",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf lib && mkdir -p lib && babel index.js > lib/index.js",
    "prepublish": "npm run build",
    "test": "ava",
    "test:watch": "npm test -- --watch",
    "lint": "standard index.js test.js",
    "pretest": "npm run lint"
  },
  "files": ["lib"],
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "standard": "^8.6.0"
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "lodash.reject": "^4.6.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "latest",
      "stage-0"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  }
}
